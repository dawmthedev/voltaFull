{
  "name": "Volta-Fullstack",
  "private": true,
  "workspaces": [
    "server",
    "client",
    "client-2"
  ],
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development npm run server\" \"npm run client-new\"",
    "start": "npm run dev",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "client-2": "cd client-2 && npm run dev",
    "test": "jest --config=jest.config.js --json --outputFile=jest-results.json"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/testing-library__jest-dom": "^6.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4"
  },
  "volta": {
    "node": "18.20.2",
    "npm": "10.9.2"
  }
}
